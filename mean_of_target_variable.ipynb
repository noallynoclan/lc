   "source": [
    "import numpy as np\n",
    "\n",
    "def mean_of_target_variable(guess, score):\n",
    "    return (score + np.log(1 - guess)) / np.log(1 / guess - 1)\n",
    "\n",
    "guess = 0.3\n",
    "score = 1.01\n",
    "\n",
    "mean_of_target_variable(guess, score)"
   ]
